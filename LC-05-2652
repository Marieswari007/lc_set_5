int sumOfMultiples(int n) {
    int c=0,e=0,f=0;
    for(int i=0;i<=n;i++){
        if(i%3==0){
            c+=i;
        }
        else if(i%5==0){
            e+=i;
        }
        else if(i%7==0){
            f+=i;
        }
        else{
           continue;
            
        }

    }
    return (c+e+f);
}
